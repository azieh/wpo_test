{
  "name": "web_performance_optimization",
  "version": "1.0.0",
  "description": "Web Performance Optimization Workshop ",
  "main": "server.js",
  "scripts": {
    "watch": "parcel watch index.html",
    "build": "parcel build index.html",
    "build:static": "npm-run-all build inline:css",
    "start:prod": "node server.js",
    "start:dev": "nodemon server.js",
    "prod": "npm-run-all build inline:css start:prod",
    "dev": "npm-run-all --parallel watch start:dev",
    "generate:eot": "ls fonts/**/*.ttf | sed -e 's/\\.ttf//g' | xargs -I % ttf2eot %.ttf %.eot",
    "generate:woff": "ls fonts/**/*.ttf | sed -e 's/\\.ttf//g' | xargs -I % ttf2woff %.ttf %.woff",
    "generate:woff2": "ls fonts/**/*.ttf | sed -e 's/\\.ttf//g' | xargs -I % sh -c \"cat %.ttf | ttf2woff2 > %.woff2\"",
    "fonts:generate": "npm-run-all --parallel generate:*",
    "fonts:subset": "node scripts/fontSubset.js",
    "optimise:jpeg": "node scripts/optimiseJpeg.js",
    "optimise:webp": "node scripts/jpegToWebP.js",
    "generate:sprites": "svg-sprite --css --css-render-css --css-dest=css --css-sprite=../images/icons.svg --css-layout=diagonal images/icons/*.svg",
    "print:selectors": "longest-selectors css/styles.css",
    "inline:css": "style-in dist/index.html > dist/index.critical.html",
    "test": "webpagetest test https://mindventures.netlify.com/index.critical.html --key=${WPT_KEY} --runs=3 --first --poll --timeout 120 --location='ec2-us-east-1:Chrome' --specs=test/testspecs.json"
  },
  "author": "Mateusz Kwasniewski <kwasniewski.mateusz@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "blazy": "1.8.2",
    "compression": "1.7.3",
    "express": "4.16.3",
    "jquery": "3.3.1",
    "mime": "2.3.1",
    "shrink-ray-current": "2.1.2"
  },
  "devDependencies": {
    "fontmin": "0.9.7-beta",
    "imagemin": "6.0.0",
    "imagemin-jpeg-recompress": "5.1.0",
    "imagemin-webp": "4.1.0",
    "longest-selectors": "1.1.0",
    "nodemon": "1.18.4",
    "npm-run-all": "4.1.3",
    "parcel-bundler": "1.10.1",
    "parcel-plugin-lazy": "0.0.4",
    "parcel-plugin-netlifypush": "file:./scripts/netlifypush/",
    "style-in": "1.2.1",
    "svg-sprite": "1.5.0",
    "ttf2eot": "2.0.0",
    "ttf2woff": "2.0.1",
    "ttf2woff2": "2.0.3",
    "webpagetest": "0.3.9"
  }
}
